{
  "name": "@mrspartak/config",
  "repository": "https://github.com/mrspartak/config",
  "version": "0.0.9",
  "description": "Config facade for TS",
  "private": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "build:clean": "rm -rf dist*",
    "test": "pnpm build && vitest",
    "check:format": "biome format --write ./src",
    "check:lint": "biome lint ./src",
    "local:node": "tsx local/main.ts"
  },
  "author": "Spartak Kagramanyan",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.8.0",
    "@types/node": "^20.14.2",
    "lefthook": "^1.6.15",
    "tsup": "^8.1.0",
    "tsx": "^4.15.4",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "valibot": "^0.31.1"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "files": ["dist"],
  "publishConfig": {
    "access": "public"
  }
}
